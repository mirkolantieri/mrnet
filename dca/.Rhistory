setwd("~/Documenti/GitHub/mrnet/dca/")
# Complex Model AUC
meniscus_lab <- read.csv2('./results (model auc)/Complex/complex-meniscus-label.csv', sep = ',')
meniscus_pred <- read.csv2('./results (model auc)/Complex/complex-meniscus-prediction.csv', sep = ',')
meniscus <- merge(meniscus_lab, meniscus_pred, by="Case", all=TRUE)
library(ROCit)
ROCit_obj <- rocit(score= meniscus$Score.y,class= meniscus$Score.x)
plot(ROCit_obj)
# ROC CURVE WITH PRECISION - RECALL
library(precrec)
precrec_obj <- evalmod(scores = meniscus$Score.y, labels = meniscus$Score.x, mode="rocprc")
plot(precrec_obj)
ggplot2::ggplot(precrec_obj)
ggplot2::ggplot(precrec_obj,data = meniscus)
ggplot2::ggplot(precrec_obj,data = meniscus,mapping = aes())
ROCit_obj <- rocit(score= meniscus$Score.y,class= meniscus$Score.x, method = "empirical")
plot(ROCit_obj)
plot(ROCit_obj$AUC)
plot(ROCit_obj$Cutoff)
print(ROCit_obj$Cutoff)
print(ROCit_obj$TPR)
table(ROCit_obj$TPR)
plot(ROCit_obj)
plot(ROCit_obj, title(main = "KNP"))
